  rdf:type owl:ObjectProperty ;
  rdfs:domain RBO:OccuringFunctionality ;
  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          RBO:Function
          RBO:System
        ) ;
    ] ;
  rdfs:subPropertyOf RBO:isRelatedToAttribute ;
.
RBO:isRelatedToSubject
