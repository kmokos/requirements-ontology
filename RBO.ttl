  rdfs:range [
      rdf:type owl:Class ;
      owl:unionOf (
          RBO:Function
          RBO:System
          RBO:Interface
        ) ;
    ] ;
  rdfs:subPropertyOf RBO:isRelatedToAttribute ;
.
RBO:isRelatedToSubjectItem
