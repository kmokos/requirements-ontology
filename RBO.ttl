RBO:contradictsWith_rule0
  rdf:type sh:SPARQLRule ;
  sh:construct """PREFIX RBO:<http://delab.csd.auth.gr/ontologies/2018/RBO#>
PREFIX LO:<http://delab.csd.auth.gr/ontologies/2018/LO#>
CONSTRUCT {
    $this RBO:contradictsWith ?Lexical .
}
WHERE {
        $this LO:antonymOf ?Lexical .
        FILTER ($this != ?Lexical)
}""" ;
  sh:order "9"^^xsd:decimal ;
  sh:prefixes LO: ;
  sh:prefixes RBO: ;
.
RBO:contradictsWith_rule1
  rdf:type sh:SPARQLRule ;
  sh:construct """PREFIX RBO:<http://delab.csd.auth.gr/ontologies/2018/RBO#>
PREFIX LO:<http://delab.csd.auth.gr/ontologies/2018/LO#>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
CONSTRUCT {
    ?this RBO:contradictsWith ?e2 .
}
WHERE {
    {?this ?pr ?val1 . ?pr rdfs:subPropertyOf* RBO:isRelatedToPlaceholder}
    ?e2 rdf:type/rdfs:subClassOf* RBO:Placeholder
    FILTER (?this != ?e2) .
    NOT EXISTS {
            $this (RBO:isSimilarTo|^RBO:isSimilarTo)+ ?e2 .
    }
    NOT EXISTS {
            $this RBO:contradictsWith ?e2 .
    }
    ?e2 ?pr ?val2 .
    ?val1 RBO:contradictsWith ?val2 .
    NOT EXISTS {
        {?this ?prop ?val . ?prop rdfs:subPropertyOf* RBO:isRelatedToPlaceholder}
        FILTER ((?prop != ?pr) && (?prop != RBO:isRelatedToPlaceholder)) .
        NOT EXISTS {
            {
                ?e2 ?prop ?val .
            }
            UNION
            {
                ?e2 ?prop ?val3 .
                ?val3 (RBO:isSimilarTo|^RBO:isSimilarTo)+ ?val .
            }
            UNION
            {
                ?e2 ?prop ?val3 .
                ?val3 RBO:contradictsWith ?val .
            } .
        } .
    } .
    NOT EXISTS {
        {?e2 ?prop ?val . ?prop rdfs:subPropertyOf* RBO:isRelatedToPlaceholder}
        FILTER ((?prop != ?pr) && (?prop != RBO:isRelatedToPlaceholder)) .
        NOT EXISTS {
            {
                ?this ?prop ?val .
            }
            UNION
            {
                ?this ?prop ?val3 .
                ?val3 (RBO:isSimilarTo|^RBO:isSimilarTo)+ ?val .
            }
            UNION
            {
                ?this ?prop ?val3 .
                ?val3 RBO:contradictsWith ?val .
            } .
        } .
    } .
}""" ;
  sh:order "4"^^xsd:decimal ;
  sh:prefixes LO: ;
  sh:prefixes RBO: ;
  sh:prefixes rdf: ;
  sh:prefixes rdfs: ;
.
RBO:contradictsWith_rule2
  rdf:type sh:SPARQLRule ;
  sh:construct """PREFIX RBO:<http://delab.csd.auth.gr/ontologies/2018/RBO#>
PREFIX LO:<http://delab.csd.auth.gr/ontologies/2018/LO#>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
CONSTRUCT {
    $this RBO:contradictsWith ?numerical .    
}
WHERE {
{
    $this rdf:type/rdfs:subClassOf* RBO:Numerical .
    ?numerical rdf:type/rdfs:subClassOf* RBO:Numerical .
    FILTER(?numerical != $this)
    $this RBO:isRelatedToNumber ?N .
    $this RBO:isRelatedToNumberUnit ?NumberUnit1 .
    {
        SELECT ?NumberUnit1 ?NumberUnit2 ?number
        {
            {
                SELECT ?NumberUnit1 ?NumberUnit2 (group_concat(?Number2/?Number3)  as ?eqNumber1) 
                { 
                    {
                        $NumberUnit1 LO:equivalentToUOM* ?EquivalentNumberUnit .
                        ?EquivalentNumberUnit LO:equivalentToUOM+ ?NumberUnit2 .
                        ?EquivalentNumberUnit LO:equivalentWithNumerator ?Number2 .
                        ?EquivalentNumberUnit LO:equivalentWithDenominator ?Number3 .
                    }
                    UNION
                    {
                        SELECT ?NumberUnit1 ?NumberUnit2 ?Number2
                        {
                            $NumberUnit1 rdf:type/rdfs:subClassOf* LO:UOM .
                            BIND ($NumberUnit1 as $NumberUnit2)
                            BIND (\"\" as ?Number2)
                        }
                    }
                }
                GROUP BY ?NumberUnit1 ?NumberUnit2
            }
            BIND (if(?eqNumber1, strAfter(?eqNumber1, ' '), ?undef) AS ?eqNumber2)
            BIND (if(?eqNumber2, strAfter(?eqNumber2, ' '), ?undef) AS ?eqNumber3)
            BIND (if(?eqNumber3, strAfter(?eqNumber3, ' '), ?undef) AS ?eqNumber4)
            BIND (if(?eqNumber4, strAfter(?eqNumber4, ' '), ?undef) AS ?eqNumber5)

            BIND (if(?eqNumber1, if(contains(?eqNumber1, ' '), strBefore(?eqNumber1, ' '), ?eqNumber1), ?undef) AS ?p1)
            BIND (if(?eqNumber2, if(contains(?eqNumber2, ' '), strBefore(?eqNumber2, ' '), ?eqNumber2), ?undef) AS ?p2)
            BIND (if(?eqNumber3, if(contains(?eqNumber3, ' '), strBefore(?eqNumber3, ' '), ?eqNumber3), ?undef) AS ?p3)
            BIND (if(?eqNumber4, if(contains(?eqNumber4, ' '), strBefore(?eqNumber4, ' '), ?eqNumber4), ?undef) AS ?p4)
            BIND (if(?eqNumber5, if(contains(?eqNumber5, ' '), strBefore(?eqNumber5, ' '), ?eqNumber5), ?undef) AS ?p5)

            BIND (COALESCE(xsd:float(?p1), 1) AS ?number1)
            BIND (COALESCE(xsd:float(?p2), 1) AS ?number2)
            BIND (COALESCE(xsd:float(?p3), 1) AS ?number3)
            BIND (COALESCE(xsd:float(?p4), 1) AS ?number4)
            BIND (COALESCE(xsd:float(?p5), 1) AS ?number5)

            BIND (?number1 * ?number2 * ?number3 * ?number4 * ?number5 AS ?number)
        }
    }

    BIND((?N*?number) AS ?EquivalentNumber) .
    ?numerical RBO:isRelatedToNumber ?otherNumber .
    ?numerical RBO:isRelatedToNumberUnit ?NumberUnit2 .
    FILTER (?EquivalentNumber != ?otherNumber)
}
UNION
{
    $this rdf:type/rdfs:subClassOf* RBO:Numerical .
    ?numerical rdf:type/rdfs:subClassOf* RBO:Numerical .
    FILTER(?numerical != $this)
    $this RBO:isRelatedToNumber ?number .
    NOT EXISTS {$this RBO:isRelatedToNumberUnit ?NumberUnit1} .
    ?numerical RBO:isRelatedToNumber ?otherNumber .
    NOT EXISTS {?numerical RBO:isRelatedToNumberUnit ?NumberUnit2} .
    FILTER (?number != ?otherNumber)
}
}""" ;
  sh:order "5"^^xsd:decimal ;
  sh:prefixes LO: ;
  sh:prefixes RBO: ;
  sh:prefixes rdf: ;
  sh:prefixes rdfs: ;
  sh:prefixes xsd: ;
.
