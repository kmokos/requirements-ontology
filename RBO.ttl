RBO:isSimilarTo
  rdf:type owl:SymmetricProperty ;
  rdf:type owl:TransitiveProperty ;
  rdfs:domain RBO:Placeholder ;
.
RBO:isSimilarTo_rule0
  rdf:type sh:SPARQLRule ;
  sh:construct """PREFIX RBO:<http://delab.csd.auth.gr/ontologies/2018/RBO#>
PREFIX LO:<http://delab.csd.auth.gr/ontologies/2018/LO#>
CONSTRUCT {
    $this RBO:isSimilarTo ?Lexical .    
    ?Lexical RBO:isSimilarTo $this .    
}
WHERE {
        $this (LO:synonymOf|^LO:synonymOf)* ?Lexical .
        FILTER ($this != ?Lexical)
}""" ;
  sh:order "10"^^xsd:decimal ;
  sh:prefixes LO: ;
  sh:prefixes RBO: ;
.
RBO:isSimilarTo_rule1
  rdf:type sh:SPARQLRule ;
  sh:construct """PREFIX RBO:<http://delab.csd.auth.gr/ontologies/2018/RBO#>
PREFIX LO:<http://delab.csd.auth.gr/ontologies/2018/LO#>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
CONSTRUCT {
    $this RBO:isSimilarTo ?e2 .
}
WHERE {
    {$this ?pr1 ?val1 . ?pr1 rdfs:subPropertyOf* RBO:isRelatedToPlaceholder}
    ?e2 rdf:type/rdfs:subClassOf* RBO:Placeholder .
    FILTER ($this != ?e2) .
    NOT EXISTS {
            $this RBO:isSimilarTo ?e2 .
    }
    {?e2 ?pr2 ?val2 . ?pr2 rdfs:subPropertyOf* RBO:isRelatedToPlaceholder}
    NOT EXISTS {
        {$this ?prop ?val . ?prop rdfs:subPropertyOf* RBO:isRelatedToPlaceholder}
        NOT EXISTS {
            {
                ?e2 ?prop ?val .
            }
            UNION
            {
                ?e2 ?prop ?val2 .
                EXISTS {
                    {
                        ?val2 RBO:isSimilarTo ?val
                    }
                    UNION 
                    {
                        FILTER (?val2 = ?val)
                    }
                    UNION {
                        ?val2 LO:synonymOf ?val
                    }
                }
            } .
        } .
    } .
    NOT EXISTS {
        {?e2 ?prop ?val . ?prop rdfs:subPropertyOf* RBO:isRelatedToPlaceholder}
        NOT EXISTS {
            {
                ?this ?prop ?val .
            }
            UNION
            {
                $this ?prop ?val2 .
                EXISTS {
                    {
                        ?val2 RBO:isSimilarTo ?val
                    }
                    UNION 
                    {
                        FILTER (?val2 = ?val)
                    }
                    UNION {
                        ?val2 LO:synonymOf ?val
                    }
                }
            } .
        } .
    } .
}""" ;
  sh:order "6"^^xsd:decimal ;
  sh:prefixes LO: ;
  sh:prefixes RBO: ;
  sh:prefixes rdf: ;
  sh:prefixes rdfs: ;
.
RBO:isSimilarTo_rule2
  rdf:type sh:SPARQLRule ;
  sh:construct """PREFIX RBO:<http://delab.csd.auth.gr/ontologies/2018/RBO#>
PREFIX LO:<http://delab.csd.auth.gr/ontologies/2018/LO#>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
CONSTRUCT {
    $this RBO:isSimilarTo ?numerical .    
    ?numerical RBO:isSimilarTo $this .    
}
WHERE {
    $this rdf:type/rdfs:subClassOf* RBO:Numerical .
    ?numerical rdf:type/rdfs:subClassOf* RBO:Numerical .
    FILTER(?numerical != $this)
    $this RBO:isRelatedToNumber ?N .
    $this RBO:isRelatedToNumberUnit ?NumberUnit1 .
    
    {
        SELECT ?NumberUnit1 ?NumberUnit2 ?number
        {
            {
                SELECT ?NumberUnit1 ?NumberUnit2 (group_concat(?Number2/?Number3)  as ?eqNumber1) 
                { 
                    {
                        $NumberUnit1 LO:equivalentToUOM* ?EquivalentNumberUnit .
                        ?EquivalentNumberUnit LO:equivalentToUOM+ ?NumberUnit2 .
                        ?EquivalentNumberUnit LO:equivalentWithNumerator ?Number2 .
                        ?EquivalentNumberUnit LO:equivalentWithDenominator ?Number3 .
                    }
                    UNION
                    {
                        SELECT ?NumberUnit1 ?NumberUnit2 ?Number2
                        {
                            $NumberUnit1 rdf:type/rdfs:subClassOf* LO:UOM .
                            BIND ($NumberUnit1 as $NumberUnit2)
                            BIND (\"\" as ?Number2)
                        }
                    }
                }
                GROUP BY ?NumberUnit1 ?NumberUnit2
            }
            BIND (if(?eqNumber1, strAfter(?eqNumber1, ' '), ?undef) AS ?eqNumber2)
            BIND (if(?eqNumber2, strAfter(?eqNumber2, ' '), ?undef) AS ?eqNumber3)
            BIND (if(?eqNumber3, strAfter(?eqNumber3, ' '), ?undef) AS ?eqNumber4)
            BIND (if(?eqNumber4, strAfter(?eqNumber4, ' '), ?undef) AS ?eqNumber5)

            BIND (if(?eqNumber1, if(contains(?eqNumber1, ' '), strBefore(?eqNumber1, ' '), ?eqNumber1), ?undef) AS ?p1)
            BIND (if(?eqNumber2, if(contains(?eqNumber2, ' '), strBefore(?eqNumber2, ' '), ?eqNumber2), ?undef) AS ?p2)
            BIND (if(?eqNumber3, if(contains(?eqNumber3, ' '), strBefore(?eqNumber3, ' '), ?eqNumber3), ?undef) AS ?p3)
            BIND (if(?eqNumber4, if(contains(?eqNumber4, ' '), strBefore(?eqNumber4, ' '), ?eqNumber4), ?undef) AS ?p4)
            BIND (if(?eqNumber5, if(contains(?eqNumber5, ' '), strBefore(?eqNumber5, ' '), ?eqNumber5), ?undef) AS ?p5)

            BIND (COALESCE(xsd:float(?p1), 1) AS ?number1)
            BIND (COALESCE(xsd:float(?p2), 1) AS ?number2)
            BIND (COALESCE(xsd:float(?p3), 1) AS ?number3)
            BIND (COALESCE(xsd:float(?p4), 1) AS ?number4)
            BIND (COALESCE(xsd:float(?p5), 1) AS ?number5)

            BIND (?number1 * ?number2 * ?number3 * ?number4 * ?number5 AS ?number)
        }
    }

    BIND((?N*?number) AS ?EquivalentNumber) .
    ?numerical RBO:isRelatedToNumber ?EquivalentNumber .
    ?numerical RBO:isRelatedToNumberUnit ?NumberUnit2 .
}""" ;
  sh:order "7"^^xsd:decimal ;
  sh:prefixes LO: ;
  sh:prefixes RBO: ;
  sh:prefixes rdf: ;
  sh:prefixes rdfs: ;
  sh:prefixes xsd: ;
.
RBO:isSimilarTo_rule3
  rdf:type sh:SPARQLRule ;
  sh:construct """PREFIX RBO:<http://delab.csd.auth.gr/ontologies/2018/RBO#>
PREFIX LO:<http://delab.csd.auth.gr/ontologies/2018/LO#>
PREFIX rdfs:<http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf:<http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
CONSTRUCT {
    $this RBO:isSimilarTo ?placeholder .
}
WHERE {
{SELECT $this ?placeholder ?NumberOfPlaceholders1 (COUNT(?pr) as ?counter)
WHERE {
    $this ?pr ?anotherPlaceholder.
    ?pr rdfs:subPropertyOf* RBO:isRelatedToPlaceholder .
    {
        {
            ?anotherPlaceholder (RBO:isSimilarTo|^RBO:isSimilarTo)* ?similarPlaceholder.
        }
        UNION
        {
            FILTER(?similarPlaceholder =?anotherPlaceholder )
        }
    }
    ?placeholder ?pr ?similarPlaceholder.
    FILTER($this != ?placeholder)
    {
        SELECT ?placeholder (COUNT(?pr1) as ?NumberOfPlaceholders1)
        WHERE {
            ?placeholder ?pr1 ?anotherPlaceholder1.
            ?pr1 rdfs:subPropertyOf* RBO:isRelatedToPlaceholder .
        }
        GROUP BY ?placeholder
    }
    {
        SELECT $this (COUNT(?pr2) as ?NumberOfPlaceholders2)
        WHERE {
            $this ?pr2 ?anotherPlaceholder2.
            ?pr2 rdfs:subPropertyOf* RBO:isRelatedToPlaceholder .
        }
        GROUP BY $this
    }
    FILTER (?NumberOfPlaceholders1 = ?NumberOfPlaceholders2)
}
GROUP BY $this ?placeholder ?NumberOfPlaceholders1
HAVING (?counter =?NumberOfPlaceholders1)
}
}""" ;
  sh:order "8"^^xsd:decimal ;
  sh:prefixes LO: ;
  sh:prefixes RBO: ;
  sh:prefixes rdf: ;
  sh:prefixes rdfs: ;
  sh:prefixes xsd: ;
.
RBO:syntaxOrder
  rdf:type owl:DatatypeProperty ;
  rdfs:domain sh:PropertyShape ;
  rdfs:range xsd:integer ;
.
