RBO:isRelatedtoUOM
  rdf:type owl:ObjectProperty ;
  rdfs:domain [
      rdf:type owl:Class ;
      owl:unionOf (
          RBO:Quantity
          RBO:M1
          RBO:M3
          RBO:M4
          RBO:S1
          RBO:S3
        ) ;
    ] ;
  rdfs:range <http://purl.oclc.org/NET/muo/muo#UnitOfMeasurement> ;
  rdfs:subPropertyOf RBO:isRelatedToAttribute ;
.
RBO:isSimilarTo
